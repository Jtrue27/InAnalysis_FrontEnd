<script>
  var app = null;

  function initApp(initTag){
    app = new Vue({
      el: '#app',
      data: {
        nowTag: initTag,
        selectorTouching: false,
        selectAngle: -90,
        selections: [
          {
            id: 'company',
            showName: '公司理念',
          },{
            id: 'responsity',
            showName: '社會責任',
          },{
            id: 'member',
            showName: '團隊成員',
          },{
            id: 'service',
            showName: '提供服務',
          },{
            id: 'achievement',
            showName: '公司成果',
          },{
            id: 'contact',
            showName: '聯絡我們',
          },{
            id: 'recruit',
            showName: '招兵買馬',
          },{
            id: 'cancel',
            showName: '取消選擇',
          },
        ],
      },
      watch: {
				nowTag: function(val){
					let realTag = this.selections[0].id;
					for(let key in this.selections){
						if(this.selections[key].id == val){
							realTag = val;
							break;
						}
					}
					this.nowTag = realTag;
					window.location.hash = '#'+this.nowTag;
				},
      },
      updated: function(){

      },
      computed: {
        selectingTag: function(){
          if(this.selectAngle > 247.5 || this.selectAngle < -67.5){
            return 'company';
          }else if(this.selectAngle < -22.5){
            return 'responsity';
          }else if(this.selectAngle < 22.5){
            return 'member';
          }else if(this.selectAngle < 67.5){
            return 'service';
          }else if(this.selectAngle < 112.5){
            return 'cancel';
          }else if(this.selectAngle < 157.5){
            return 'achievement';
          }else if(this.selectAngle < 202.5){
            return 'contact';
          }else if(this.selectAngle < 247.5){
            return 'recruit';
          }
          return '';
        },
        selectingTagName: function(){
          for(var s in this.selections){
            const sel = this.selections[s];
            if(sel.id == this.selectingTag){
              return sel.showName;
            }
          }
          return '';
        },
        nowTagObject: function(){
          for(var s in this.selections){
            const sel = this.selections[s];
            if(sel.id == this.nowTag){
              return sel;
            }
          }
          return {};
        },
      },
      methods: {
        onTouchStart: function(event){
          event.preventDefault();
          this.selectorTouching = true;
        },
        onTouchEnd: function(event){
          event.preventDefault();
          this.selectorTouching = false;
          if(this.selectingTag != 'cancel'){
            this.nowTag = this.selectingTag;
          }
        },
        onTouchMove: function(event){
          event.preventDefault();
          var w = window.screen.width;
          var h = window.screen.height;
          var touchPoint = event.touches[0];

          var x_d = touchPoint.clientX - w/2;
          var y_d = touchPoint.clientY - h/2;
          var deg = Math.atan(y_d / x_d) / Math.PI * 180;
          if(x_d < 0){
            deg = 180+deg;
          }
          this.selectAngle = deg;
          deg += 45;
          $('.selector-btn-center').css('transform', 'translate(-50%, -50%) rotate('+deg+'deg)');
        },
      }
    });
  }

	function toggleRecruit(id){
		var $outerDiv = $("#"+id);
		if($outerDiv.attr('cous') == 'opened'){
			$outerDiv.attr('cous', 'working');
			$outerDiv.find('.open-btn').show();
			$outerDiv.find('.close-btn').hide();
			$outerDiv.find('.recruit-desc').slideUp(500, function(){
				$outerDiv.attr('cous', 'closed');
			});
		}else if($outerDiv.attr('cous') == 'closed'){
			$outerDiv.attr('cous', 'working');
			$outerDiv.find('.close-btn').show();
			$outerDiv.find('.open-btn').hide();
			$outerDiv.find('.recruit-desc').slideDown(500, function(){
				$outerDiv.attr('cous', 'opened');
			});
		}else if($outerDiv.attr('cous') == 'working'){
		}
	}

  $(document).ready(function(){
		var initTag = 'company';
		if(window.location.hash.length > 0){
			initTag = window.location.hash.substr(1);
		}
		initApp(initTag);

  });
</script>
